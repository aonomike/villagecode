 
        <div class="main-container"> 
            <section class="page-title page-title-4 bg-secondary"> 
                <div class="container"> 
                    <div class="row"> 
                        <div class="col-md-6"> 
                            <h3 class="uppercase mb0">Application Users</h3> 
                        </div> 
                        <div class="col-md-6 text-right"> 
                            <ol class="breadcrumb breadcrumb-2"> 
                                <li> 
                                    <a href="/">Village Code</a> 
                                </li> 
                                <li class="active">Users</li> 
                            </ol> 
                        </div> 
                    </div> 
                    <!--end of row--> 
                </div> 
                <!--end of container--> 
            </section> 
            <section> 
                <div class="container"> 
                    <div class="row">
                        <div class="col-md-2 col-sm-4">
                             <span class="input-lh results-count">Displaying <%=@users.size%> of <%=@users.size%> results</span>
                            <!--end select-->
                        </div>
                    </div>
                    <div class="row"> 
                        <div class="col-md-9 col-md-offset-0 col-sm-10 col-sm-offset-1"> 
                                <div id="filter_results_div">
                                    <table class="table cart mb48"> 
                                        <thead> 
                                            <tr> 
                                                <th>&nbsp;</th> 
                                                <th></th> 
                                                <th>User</th> 
                                                <th>Email</th> 
                                                <th>Role</th> 
                                                <th>Log In Count</th> 
                                            </tr> 
                                        </thead>                                 
                                        <tbody> 
                                            <% @users.each do |user| %>
                                            <tr>
                                              <%= render user %>
                                            </tr>
                                            <%end%>
                                        </tbody> 
                                    </table> 
                                </div>
                        </div> 
                        <!--end of items--> 
                        <div class="col-md-3 col-md-offset-0 col-sm-10 col-sm-offset-1"> 
                            <div class="mb24"> 
                                <form> 
                                    <h5 class="uppercase">Search</h5> 
                                    <input type="text" placeholder="Enter name/email" /> 
                                </form>

                                <h5 class="uppercase">Filter</h5> 
                                <div class="select-option"> 
                                    <i class="ti-angle-down"></i> 
                                    <select id="role_type"> 
                                        <option selected value="Default">Select Role</option> 
                                        <option value="super_admin">Super Admin</option> 
                                        <option value="blog_admin">Blog Admin</option> 
                                        <option value="blog_team">Blog Team</option> 
                                        <option value="guest">Guest</option> 
                                    </select> 
                                </div> 
                                <!--end select--> 
                                 
                            </div> 
                            <div class="mb24"> 
                                <%=render "stats"%>
                            </div> 
                        </div> 
                        <!--end of totals--> 
                    </div> 
                    <!--end of row--> 
                </div> 